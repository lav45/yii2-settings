{
    "name": "lav45/yii2-settings",
    "description": "This extension helps you to easily store and retrieve data for your application.",
    "keywords": [
        "yii2",
        "extension",
        "settings",
        "config",
        "quick access"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "homepage": "https://github.com/lav45/yii2-settings",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexey Loban",
            "email": "lav451@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "yiisoft/yii2": ">=2.0.13"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "4.8 - 7.5"
    },
    "autoload": {
        "psr-4": {
            "lav45\\settings\\": "src",
            "lav45\\settings\\tests\\": "tests\\tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
